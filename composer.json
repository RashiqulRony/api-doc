{
    "name": "rashiqulrony/api-doc",
    "license": "MIT",
    "description": "Generate beautiful API documentation from your Laravel application",
    "keywords": [
        "API",
        "Documentation",
        "Laravel"
    ],
    "homepage": "http://github.com/rashiqulrony/api-doc",
    "authors": [
        {
            "name": "Rashiqul Rony",
            "email": "rashiqulrony@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "fakerphp/faker": "^1.12",
        "illuminate/routing": "5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/support": "5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0",
        "illuminate/console": "5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0",
        "mpociot/reflection-docblock": "^1.0.1",
        "ramsey/uuid": "^3.8 || ^4.0",
        "league/fractal": "^0.20.2",
        "api-ecosystem-for-laravel/dingo-api": "^4.6"
    },
    "require-dev": {
        "orchestra/testbench": "3.5.* || 3.6.* || 3.7.* || 5.1.* || ^6.0 || ^9.0",
        "phpunit/phpunit": "^6.0.0 || ^7.4.0 || ^9.0 || ^10.5",
        "mockery/mockery": "^1.2.0"
    },
    "autoload": {
        "files": [
            "helpers/helpers.php"
        ],
        "psr-4": {
            "RashiqulRony\\ApiDoc\\": "src/apidoc"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RashiqulRony\\ApiDoc\\IDocServiceProvider"
            ]
        }
    }
}
